<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="krok.lifts">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lifts_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MaxesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".StartScreen" />
        </activity>
        <activity android:name=".WorkoutActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".StartScreen" />
        </activity>
        <activity android:name=".LiftActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WorkoutActivity" />
        </activity>
        <activity android:name=".CompoundAnalysisActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".StartScreen" />
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>